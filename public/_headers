# Cache all static assets very aggressively
/assets/*
  Cache-Control: public, max-age=31536000, immutable
  Access-Control-Allow-Origin: *

# Favicons and icons
/favicon.png
  Cache-Control: public, max-age=31536000, immutable
  Access-Control-Allow-Origin: *

/favicon.ico
  Cache-Control: public, max-age=31536000, immutable
  Access-Control-Allow-Origin: *

# Robots can be cached, but not forever
/robots.txt
  Cache-Control: public, max-age=86400
